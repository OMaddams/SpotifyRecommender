<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="SpotifyRecommender.UI.AuthPage"
             Title="AuthPage">
    <VerticalStackLayout Margin="10">
        <WebView x:Name="webViewA" Source="https://google.com" HeightRequest="540" WidthRequest="960" Navigating="webViewA_Navigating"/>


        <Label 
            Text="Welcome to .NET MAUI!"
            VerticalOptions="Center" 
            HorizontalOptions="Center"
            TextColor="White"/>
        <Button Text="Go Back" Clicked="Button_Pressed" Pressed="Button_Pressed"></Button>
    </VerticalStackLayout>
</ContentPage>